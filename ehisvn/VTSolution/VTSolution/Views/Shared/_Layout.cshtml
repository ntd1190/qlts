            });
            setbanner2018();
            $(window).on('scroll', function () {
                setbanner2018();
            });
        });
        function setbanner2018resize()
        {
            if ($(window).width() < 1440)
                $('.banner2018').hide();
            else
                $('.banner2018').show();
        }
        function setbanner2018()
        {
            var $top = $(window).scrollTop();
            let moc = 0;
            if ($('#main-home > .container-fluid').length < 1)
                moc = $('#wrapper').position().top;
            else
                moc = $('#main-home > .container-fluid').position().top + $('#main-home > .container-fluid').height();
            if ($top > moc + 20)
            {
                if ($top > $('#footer').position().top - 570)
                {
                    $('.banner2018').css({ top: $('#footer').position().top -560 });
                    $('.banner2018').addClass('fixbot').removeClass('fix');
                }
                else
                    $('.banner2018').addClass('fix').removeClass('fixbot');
            }
            else
            {
                $('.banner2018').removeClass('fix').removeClass('fixbot');
                $('.banner2018').css({ top: moc + 10 });
            }
        }
    </script>
