<script>
    var plugin_path = 'plugin/';
    var url = window.location;
    jQuery('.topmenu li').removeClass('current-menu-item');

    var element = jQuery('.topmenu li a').filter(function () {
        return this.href == url || (url.href.indexOf(this.href) > -1 && this.href.length > url.origin.length + 1);
    }).parent().addClass('current-menu-item');
    if (jQuery('#menu-item-6256').attr('value') != "") {
        jQuery('#menu-item-6256').addClass('current-menu-item');
    }
    jQuery(document).ready(function () {


        jQuery('#load').click(function () {

            //setTimeout(function () {

            //}, 2000);
            var name = jQuery('#khach').val();
            var email = jQuery('#quick-your-email').val();
            var phone = jQuery('#quick-your-phone').val();
            var subject = jQuery('#quick-your-subject').val();
            var company = jQuery('#quick-your-company').val();
            var content = jQuery('#quick-your-message').val();

            if (jQuery('#khach').val() == "") {
                jQuery('#tags').select();
                jQuery('.rpt').text("Vui lòng chọn 1 khách hàng trong danh sách").fadeIn();
                return false;
            }
            if (email == "") {
                jQuery('#quick-your-email').select();
                jQuery('.rpt').text("Vui lòng nhập địa chỉ email").fadeIn();
                return false;
            }
            if (!isValidEmailAddress(email)) {
                jQuery('#quick-your-email').select();
                jQuery('.rpt').text("Vui lòng nhập đúng định dạng địa chỉ email").fadeIn();
                return false;
            }
            if (phone == "") {
                jQuery('#quick-your-phone').select();
                jQuery('.rpt').text("Vui lòng nhập sdt").fadeIn();
                return false;
            }
            if (subject == "") {
                jQuery('#quick-your-subject').select();
                jQuery('.rpt').text("Vui lòng nhập chủ đề").fadeIn();
                return false;
            }
            if (company == "") {
                jQuery('.rpt').text("Vui lòng nhập người liên hệ").fadeIn();
                jQuery('#quick-your-company').select();
                return false;
            }
            if (content == "") {
                jQuery('.rpt').text("Vui lòng nhập nội dung").fadeIn();
                jQuery('#quick-your-message').select();
                return false;
            }
            var jQuerythis = jQuery(this);
            jQuerythis.button('loading');
            var captcharesponse = grecaptcha.getResponse();
            jQuery.ajax({
                url: '@Url.Action("SendInfo", "Home")',
                type: 'Post',
                contentType: 'application/json',
                data: JSON.stringify({ name: name, email: email, phone: phone, subject: subject, company: company, content: content, captcharesponse: captcharesponse }),
                success: function (response) {
                    jQuery('#tags').val('');
                    jQuery('#khach').val('');
                    jQuery('#quick-your-email').val('');
                    jQuery('#quick-your-phone').val('');
                    jQuery('#quick-your-company').val('');
                    jQuery('#quick-your-subject').val('');
                    jQuery('#quick-your-message').val('');
                    if (response == "1") {
                        jQuery('.rpt').text("Gửi yêu cầu hỗ trợ thành công").css("color", "#3276b1").fadeIn();
                    }
                    else {
                        jQuery('.rpt').text(response).fadeIn();
                    }
                    grecaptcha.reset();
                    jQuerythis.button('reset');
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    toastr.warning("có lỗi xảy ra,vui lòng thử lại sau");
                }
            });
        })
    });
    jQuery('#viewchitiet').click(function () {
        jQuery.ajax({
            url: '@Url.Action("GetItemListById", "Home")',
            type: 'Post',
            contentType: 'application/json',
            data: JSON.stringify({ id: $('#khach1').val(), tungay: $('#datepicker').val(), denngay: $('#datepicker1').val() }),
            success: function (response) {

                if (response.length == 0) {
                    jQuery(".listdata").empty();
                    return false;
                    gettable();

                }


                AppendList(response);
            },
            error: function (jqXHR, textStatus, errorThrown) {
                toastr.warning("có lỗi xảy ra,vui lòng thử lại sau");
            }

        })
    })
    function gettable() {
        loadScript(plugin_path + "datatables/js/jquery.dataTables.min.js", function () {
            loadScript(plugin_path + "datatables/dataTables.bootstrap.js", function () {
                if (jQuery().dataTable) {
                    var table = jQuery('#datatable_sample');
                    table.dataTable({
                        "lengthMenu": [
                            [10, 20, 30, -1],
                            [10, 20, 30, "All"] // change per page values here
                        ],
                        // set the initial value
                        "pageLength": 5,
                        "searching": false,
                        "pagingType": "bootstrap_full_number",
                        "language": {
                            "lengthMenu": "  Hiển thị _MENU_",
                            "paginate": {
                                "previous": "Prev",
                                "next": "Next",
                                "last": "Last",
                                "first": "First"
                            }
                        }
                    });



                    table.on('change', 'tbody tr .checkboxes', function () {
                        jQuery(this).parents('tr').toggleClass("active");
                    });


                }

            });
        });
    }
    function AppendList(data) {
        var append = "";
        $(".listdata").empty();
        if (data != null) {
            var i = 1;
            data.forEach(function (item) {

                append += "<tr class='odd gradeX'>";
                append += "<td class='txtth'>" + item.ngaydenghi + "</td>";
                append += "<td class='txtth'>" + item.tieude + "</td>";
                append += "<td class='txtth'>" + item.mota + "</td>";
                append += "<td class='txtth'>" + item.huongxuly + "</td>";
                append += "<td class='txtth'>" + item.nguoixuly + "</td>";
                append += "<td class='txtth'>" + item.trangthai + "</td>";
                if (item.matrangthai == "3") {
                    if (item.Email != null) {
                        append += "<td class='txtth'><button ma=" + item.IssueId + " email=" + item.Email + " company=\'" + item.NguoiLienHe + "\' class='btn btn-primary xemdg' data-loading-text='<i class=\"fa fa-spinner fa-spin\"></i> Đang xử lý'>Nhận Đánh Giá</button></td>";
                    }
                    else {
                        append += "<td class='txtth'><button class='btn btn-primary nhapemail' ma=" + item.IssueId + " company=\'" + item.NguoiLienHe + "\' >Nhập email</button></td>";
                    }

                }
                else {
                    append += "<td class='txtth'></td>";
                }
                append += "</tr>";
                i = i + 1;
            });
            $('#datatable_sample').dataTable().fnDestroy();
            $(".listdata").html(append);
            gettable();
        }
    }
