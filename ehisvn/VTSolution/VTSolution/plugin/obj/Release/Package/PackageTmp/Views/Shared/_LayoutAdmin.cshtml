<body>
    <!-- Form logout submit -->
    @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
    {
        @Html.AntiForgeryToken()

    }
    <!-- Header -->
    <header>
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0;z-index:10;">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="javascript:;">SONG ÂN-TRANG QUẢN LÝ</a>
            </div>
            <ul class="nav navbar-top-links navbar-right">

                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i> @User.Identity.Name  <i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                      
                        <li>
                            <a href="javascript:document.getElementById('logoutForm').submit()">
                                <i class="fa fa-sign-out fa-fw"></i> Logout
                            </a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="/" target="_blank"><i class="fa fa-share"></i> Webstite</a>
                </li>
            </ul>

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        @{
                            var menus = Helper.GetMenuAdmin();
                        }

                        @foreach (var m in menus)
                        {
                            <li>
                                <a href="#">
                                    <i></i> @m.MenuName<span class="fa arrow"></span>
                                </a>
                                <ul class="nav nav-second-level">
                                    @{
                            var child = Helper.GetMenuChild(m.Id);
                                    }
                                    @foreach (var c in child)
                                    {
                                        <li><a href="@Url.Action(c.View, c.Controller)"> @c.MenuName</a></li>
                                    }

                                </ul>

                            </li>

                        }

                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>
    </header>
    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">@ViewBag.Title</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <div class="row">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
    </div>

    <footer></footer>
    <script src="~/tinymce/tinymce.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="/Scripts/Boostrap/bootstrap.min.js"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="/Scripts/metisMenu/metisMenu.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="/Scripts/sbAdmin/sb-admin-2.js"></script>
    <!--Bootstrap growl notification-->
    <script src="/Scripts/jsBootstrapGrowl/jquery.bootstrap-growl.js"></script>
    <!--Multi select-->
    <script src="/Scripts/jquery-ui-1.8.24.js"></script>
    <script src="/Scripts/jsMultiSelected/jquery.bsmselect.js"></script>
    <script src="/Scripts/jsMultiSelected/jquery.bsmselect.sortable.js"></script>
    <script src="/Scripts/jsMultiSelected/jquery.bsmselect.compatibility.js"></script>
    <!--Nestable-->
    <script src="~/Scripts/jquery.nestable.js"></script>
    <script src="~/Scripts/jsForLayoutAdmin/control.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
