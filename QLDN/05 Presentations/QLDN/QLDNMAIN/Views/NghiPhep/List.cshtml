                    </tr>
                    <tr ng-repeat="loainghiphep in ctrl.data.listLoaiNghiPhep" ng-class="(loainghiphep.isSelected==true)?'info':''">
                        <td st-select-row="loainghiphep" class="text-center"><i class="fa fa-fw text-primary" ng-class="(loainghiphep.isSelected==true)?'fa-check-square-o':'fa-square-o'"></i></td>
                        <td><a ng-bind="loainghiphep.TenLoaiPhep" href="javascript:void(0);"></a></td>
                        <td><span ng-bind="loainghiphep.SoNgay"></span></td>
                        <td><span ng-bind="loainghiphep.GhiChu"></span></td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr>
                        <td colspan="100" class="text-center">
                            <div st-pagination="" st-items-by-page="10" st-template="~/Scripts/angular/shared/smart-table/template/pagination.custom.html"></div>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </div>
        <div class="panel-footer">
            <button class="btn btn-sm btn-primary" ng-click="ctrl.action.apDung()">Áp dụng</button> &nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" data-toggle="collapse" data-target="#LoaiNghiPhepListPopup2">Đóng</a>
        </div>
    </div><!-- /popup danh sách loại nghỉ phép 2 -->
    <!-- popup cập nhật nghỉ phép -->
    <div class="panel panel-primary ui-draggable collapse fade hidden-print" id="NghiPhepEditPopup" ng-controller="NghiPhepEditCtrl as ctrl" ng-init="ctrl.onInitView({controllerId:mainCtrl.controllerId.NghiPhepEditPopup,userInfo: @ViewBag.userInfo})">
        <div class="panel-heading ui-draggable-handle">
            <div class="panel-title">
                <span>Thông tin nghỉ phép</span>
                <a class="btn btn-xs btn-primary pull-right" data-toggle="collapse" data-target="#NghiPhepEditPopup" ng-click="ctrl.action.closeEdit()">
                    <i class="fa fa-fw fa-remove">
                    </i>
                </a>
            </div>
        </div>
        <div class="panel-body" style="width: 600px;height: 400px;">
            <form class="form-horizontal">

                <!-- Input row Ngày xin phép-->
                <div class="row form-group form-group-sm">
                    <label class="control-label col-sm-3">Ngày xin phép</label>
                    <div class="form-inline col-sm-9">
                        <input type="text" id="tungayedit" class="form-control datetimepicker date" placeholder="dd/mm/yyyy" ng-model="ctrl.data.objNghiPhep.TuNgay" />
                        <span>~</span>
                        <input type="text" id="denngayedit" class="form-control datetimepicker date" placeholder="dd/mm/yyyy" ng-model="ctrl.data.objNghiPhep.DenNgay" />
                    </div>
                </div>

                <!-- Input row Số ngày -->
                <div class="row form-group form-group-sm" ng-class="{'has-error':ctrl.status.isInValidSoNgay}">
                    <label class="control-label col-sm-3">Số ngày</label>
                    <div class="col-sm-9">
                        <input id="txtSoNgay" class="form-control" ng-model="ctrl.data.objNghiPhep.SoNgay" required />
                    </div>
                </div>

                <!-- Input row Loại ngày -->
                <div class="row form-group form-group-sm">
                    <label class="control-label col-sm-3">Loại ngày</label>
                    <div class="col-sm-9">
                        <label class="radio-inline">
                            <input style="margin-right: 5px;" type="radio" ng-model="ctrl.data.objNghiPhep.LoaiNgay" value="1" name="LoaiNgay" /><span>Cả ngày</span>
                        </label>
                        <label class="radio-inline">
                            <input style="margin-right: 5px;" type="radio" ng-model="ctrl.data.objNghiPhep.LoaiNgay" value="2" name="LoaiNgay" /><span>Buổi sáng</span>
                        </label>
                        <label class="radio-inline">
                            <input style="margin-right: 5px;" type="radio" ng-model="ctrl.data.objNghiPhep.LoaiNgay" value="3" name="LoaiNgay" /><span>Buổi chiều</span>
                        </label>
                    </div>
                </div>

                <!-- Input row Tiêu đề -->
                <div class="row form-group form-group-sm" ng-class="{'has-error':ctrl.status.isInValidTieuDe}">
                    <label class="control-label col-sm-3">Tiêu đề</label>
                    <div class="col-sm-9">
                        <input id="txtTieuDe" class="form-control" ng-model="ctrl.data.objNghiPhep.TieuDe" />
                    </div>
                </div>

                <!-- Input row Lý do -->
                <div class="row form-group form-group-sm" ng-class="{'has-error':ctrl.status.isInValidLyDo}">
                    <label class="control-label col-sm-3">Lý do</label>
                    <div class="col-sm-9">
                        <input id="txtLyDo" class="form-control" ng-model="ctrl.data.objNghiPhep.LyDo" />
                    </div>
                </div>

                <!-- Loai Nghi Phep -->
                <div class="row form-group form-group-sm" ng-class="{'has-error':ctrl.status.isInValidLoaiNghiPhep}">
                    <label for="txtLoaiNghiPhep" class="control-label col-sm-3">Loại Nghỉ Phép</label>
                    <div class="col-sm-9">
                        <div class="form-control auto-height" id="searchLoaiNghiPhep">
                            <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.clearListLoaiNghiPhep()"><i class="fa fa-fw fa-remove"></i></a>
                            <a href="javascript:void(0);" data-toggle="collapse" data-target="#LoaiNghiPhepListPopup2">
                                <div>
                                    <i class="fa fa-fw fa-search"></i>
                                    <span class="" style="margin-right: 5px;" ng-bind="loainghiphep.TenLoaiPhep+','" ng-repeat="loainghiphep in ctrl.data.listLoaiNghiPhep"></span>
                                </div>

                            </a>
                        </div>
                    </div>
                </div>

                <!-- Người bàn giao -->
                <div class="row form-group form-group-sm">
                    <label for="txt" class="control-label col-sm-3">Người bàn giao</label>
                    <div class="col-sm-9">
                        <div class="form-control auto-height" id="searchNguoiBanGiao">
                            <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.clearListNguoiBanGiao()"><i class="fa fa-fw fa-remove"></i></a>
                            <a href="javascript:void(0);" data-toggle="collapse" data-target="#NhanVienListPopup2">
                                <div>
                                    <i class="fa fa-fw fa-search"></i>
                                    <span class="" style="margin-right: 5px;" ng-bind="nhanvien.Ho+' '+nhanvien.Ten+','" ng-repeat="nhanvien in ctrl.data.listNguoiBanGiao"></span>
                                </div>

                            </a>
                        </div>
                    </div>
                </div>

                <!-- Người yêu cầu -->
                <div class="row form-group form-group-sm" ng-class="{'has-error':ctrl.status.isInValidNguoiYeuCau}">
                    <label for="txt" class="control-label col-sm-3">Người yêu cầu</label>
                    <div class="col-sm-9">
                        <div class="form-control auto-height" id="searchNguoiYeuCau">
                            <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.clearListNguoiYeuCau()"><i class="fa fa-fw fa-remove"></i></a>
                            <a href="javascript:void(0);" data-toggle="collapse" data-target="#NhanVienListPopup3">
                                <div>
                                    <i class="fa fa-fw fa-search"></i>
                                    <span class="" style="margin-right: 5px;" ng-bind="nhanvien.Ho+' '+nhanvien.Ten+','" ng-repeat="nhanvien in ctrl.data.listNguoiYeuCau"></span>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>

            </form>
        </div>
        <div class="panel-footer">
            <a class="btn btn-sm btn-primary" ng-click="ctrl.action.save()" ng-if="ctrl.data.showButtonSave">Lưu (F8)</a> &nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" ng-click="ctrl.action.refresh()">Thiết lập lại</a> &nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" data-toggle="collapse" data-target="#NghiPhepEditPopup" ng-click="ctrl.action.closeEdit()">Đóng</a> &nbsp;&nbsp;&nbsp;
            <button type="button" id="btnXoaId" class="btn  btn-sm btn-default" ng-click="ctrl.action.deleteOne(ctrl.data.objNghiPhep)" ng-if="ctrl.data.objNghiPhep.NghiPhepId!=0 && ctrl.data.showButtonXoa">Xóa</button>&nbsp;&nbsp;&nbsp;
            <button type="button" id="btnH" class="btn btn-sm btn-default" ng-click="ctrl.action.GetListLuocSu(ctrl.data.objNghiPhep.NghiPhepId)" ng-if="ctrl.data.objNghiPhep.NghiPhepId!=0">H</button>
        </div>
    </div><!-- /popup cập nhật nghỉ phép -->
    <!-- popup danh sách tùy chọn cột -->
    <div class="panel panel-primary ui-draggable collapse fade" id="{{::mainCtrl.controllerId.TuyChonCotPopup}}" ng-controller="TuyChonCotListCtrl as ctrl" ng-init="ctrl.onInitView({controllerId:mainCtrl.controllerId.TuyChonCotPopup,MaForm:'FL0002', showList: true})">
        <div class="panel-heading ui-draggable-handle">
            <div class="panel-title">
                <span>Tùy chọn cột</span>
                <a class="btn btn-xs btn-primary pull-right" data-toggle="collapse" data-target="#TuyChonCotPopup"><i class="fa fa-fw fa-remove"></i></a>
            </div>
        </div>
        <div class="table-responsive" style="width: 400px; height: auto;" ng-if="ctrl.data.showList">
            <table class="table table-bordered table-condensed table-hover table-striped" st-table="ctrl.data.listCot">
                <thead>
                    <tr>
                        <th style="width: 65px;">Hiện/ẩn</th>
                        <th>Tên cột</th>
                        <th>Độ rộng</th>
                    </tr>
                </thead>
                <tbody style="height:100px;">
                    <tr ng-repeat="cot in ctrl.data.listCot" ng-class="(cot.isSelected==true)?'info':''" ng-init="cot.isSelected=cot.HienThiYN">
                        <td st-select-row="cot" st-select-mode="multiple" class="text-center"><i class="fa fa-fw text-primary" ng-class="(cot.isSelected==true)?'fa-check-square-o':'fa-square-o'"></i></td>
                        <td><span ng-bind="cot.TenCot"></span></td>
                        <td><span ng-bind="cot.DoRong"></span></td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr>
                        <td colspan="100" class="text-center">
                            <div st-pagination="" st-items-by-page="10" st-template="~/Scripts/angular/shared/smart-table/template/pagination.custom.html"></div>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </div>
        <div class="panel-footer">
            <a class="btn btn-sm btn-primary" ng-click="ctrl.action.apDung()">Áp dụng</a>&nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" data-toggle="collapse" data-target="#TuyChonCotPopup">Đóng</a>
        </div>
    </div><!-- / popup danh sách tùy chọn cột -->


</div>

