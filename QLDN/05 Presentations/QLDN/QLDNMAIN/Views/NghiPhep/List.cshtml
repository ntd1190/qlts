
@{
    ViewBag.Title = "Nghỉ Phép";
}


<div class="container-fluid" ng-controller="MainCtrl as mainCtrl" ng-init="mainCtrl.onInitView({ userInfo: @ViewBag.userInfo})" keyboard="mainCtrl.keys">
    <!-- panel tìm kiếm -->
    <div class="panel panel-default hidden-print">
        <div class="panel-heading">
            <div class="panel-title clearfix">
                <span><i class="fa fa-fw fa-star-o no-margin"></i>Nghỉ Phép</span>
                <div class="btn-group btn-group-sm pull-right">
                    <a class="btn btn-primary" data-toggle="collapse" data-target="#panelTimkiemCollapse"><i class="fa fa-fw fa-search"></i>Tìn kiếm (F3)</a>
                    <a class="btn btn-warning" data-toggle="collapse" data-target="#TuyChonCotPopup"><i class="fa fa-fw fa-cog"></i>Tùy chọn</a>
                </div>
            </div>
        </div>

        <div class="collapse fade" id="panelTimkiemCollapse" ng-controller="NghiPhepFilterCtrl as ctrl" ng-init="ctrl.onInitView('NghiPhepFilterCtrl')">
            <div class="panel-body">
                <form class="form-horizontal">
                    <!-- search field -->
                    <div class="row form-group form-group-sm">
                        <label for="txtNhanVien" class="control-label col-sm-2">Ngày xin phép</label>
                        <div class="form-inline col-sm-10">
                            <input type="text" id="tungay" class="form-control datetimepicker date" placeholder="dd/mm/yyyy" />
                            <span>~</span>
                            <input type="text" id="denngay" class="form-control datetimepicker date" placeholder="dd/mm/yyyy" />
                        </div>
                    </div>

                    <!-- search field -->
                    <div class="row form-group form-group-sm">
                        <label for="txtLoaiNghiPhep" class="control-label col-sm-2">Loại Nghỉ Phép</label>
                        <div class="col-sm-10">
                            <div class="form-control auto-height">
                                <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.clearListLoaiNghiPhep()"><i class="fa fa-fw fa-remove"></i></a>
                                <a href="javascript:void(0);" data-toggle="collapse" data-target="#LoaiNghiPhepListPopup">
                                    <div>

                                        <i class="fa fa-fw fa-search"></i>
                                        <span class="" style="margin-right: 5px;" ng-bind="loainghiphep.TenLoaiPhep+','" ng-repeat="loainghiphep in ctrl.data.listLoaiNghiPhep"></span>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>

                    <!-- search field -->
                    <div class="row form-group form-group-sm">
                        <label for="txtNhanVien" class="control-label col-sm-2">Trạng thái</label>
                        <div class="form-inline col-sm-10">
                            <div class="checkbox" style="margin-right: 15px;">
                                <label>
                                    <input style="margin-right: 5px;" type="checkbox" ng-click="ctrl.action.checkTrangThaiTatCa()" ng-model="ctrl.data.trangThai.tatCa" /><span>Tất cả</span>
                                </label>
                            </div>
                            <div class="checkbox" style="margin-right: 5px;">
                                <label>
                                    <input style="margin-right: 5px;" type="checkbox" ng-click="ctrl.action.checkTrangThai()" ng-model="ctrl.data.trangThai.doiDuyet" /><span>Đợi duyệt</span>
                                </label>
                            </div>
                            <div class="checkbox" style="margin-right: 5px;">
                                <label>
                                    <input style="margin-right: 5px;" type="checkbox" ng-click="ctrl.action.checkTrangThai()" ng-model="ctrl.data.trangThai.dongY" /><span>Đồng ý</span>
                                </label>
                            </div>
                            <div class="checkbox" style="margin-right: 5px;">
                                <label>
                                    <input style="margin-right: 5px;" type="checkbox" ng-click="ctrl.action.checkTrangThai()" ng-model="ctrl.data.trangThai.tuChoi" /><span>Từ chối</span>
                                </label>
                            </div>
                        </div>
                    </div>

                    <!-- search field -->
                    <div class="row form-group form-group-sm">
                        <label for="txt" class="control-label col-sm-2">Người duyệt</label>
                        <div class="col-sm-10">
                            <div class="form-control auto-height">
                                <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.clearListNguoiDuyet()"><i class="fa fa-fw fa-remove"></i></a>
                                <a href="javascript:void(0);" data-toggle="collapse" data-target="#NhanVienListPopup">
                                    <div>
                                        <i class="fa fa-fw fa-search"></i>
                                        <span class="" style="margin-right: 5px;" ng-bind="nhanvien.Ho+' '+nhanvien.Ten+','" ng-repeat="nhanvien in ctrl.data.listNguoiDuyet"></span>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>

                </form>
            </div>

            <div class="panel-footer">
                <a class="btn btn-sm btn-primary" ng-click="ctrl.action.search()">Tìm kiếm (F8)</a> &nbsp;&nbsp;&nbsp;
                <a class="btn btn-sm btn-default" ng-click="ctrl.action.reset()">Thiết lặp lại</a>
            </div>
        </div>
    </div><!-- / panel tìm kiếm -->
    <!-- panel Danh sách nghỉ phép -->
    <div class="panel panel-default" ng-controller="NghiPhepListCtrl as ctrl" ng-init="ctrl.onInitView({controllerId:'NghiPhepListCtrl',loadListCot:true,userInfo: @ViewBag.userInfo})">


        <div class="panel-body">
            <h3 class="text-center no-margin">Danh sách nghỉ phép</h3>
        </div>


        <div class="table-responsive">
            <table class="table table-bordered table-condensed table-responsive table-hover " st-table="ctrl.data.listNghiPhep" st-pipe="ctrl.action.getPage">
                <thead class="bg-default text-primary">
                    <tr>
                        <th style="width: 50px;"></th>
                        <th class="text-center" st-sort="{{cot.MaCot}}" ng-repeat="cot in ctrl.data.listCot|filter:cot.HienThiYN=true" style="width:{{cot.DoRong}}">{{cot.TenCot}}</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-if="ctrl.data.isLoading">
                        <td colspan="100" class="text-center info">
                            <i class="fa fa-fw fa-spinner fa-pulse"></i>
                        </td>
                    </tr>
                    <tr ng-if="ctrl.data.error.code>0">
                        <td colspan="100" class="text-center danger">
                            <span ng-bind="{{ctrl.data.error.message}}"></span>
                        </td>
                    </tr>
                    <tr ng-repeat="nghiphep in ctrl.data.listNghiPhep" ng-class="(nghiphep.isSelected==true)?'info':''">
                        <td class="text-primary text-center" st-select-row="nghiphep" st-select-mode="multiple"><i class="fa fa-fw" ng-class="nghiphep.isSelected==true?'fa-check-square-o':'fa-square-o'"></i></td>
                        <td ng-repeat="cot in ctrl.data.listCot|filter:cot.chon=true" ng-switch="cot.MaCot" ng-class="(ctrl.action.checkInValid(nghiphep[cot.MaCot], 'isNumber')==true)?'text-left':'text-right'">
                            <a href="javascript:void(0)" ng-click="ctrl.action.xemNghiPhep(nghiphep.NP_ID)" ng-switch-when="TieuDe" ng-bind="::nghiphep[cot.MaCot]"></a>
                            <a href="javascript:void(0)" ng-click="ctrl.action.xemNghiPhep(nghiphep.NP_ID)" ng-switch-when="HoTenNV" ng-bind="::nghiphep[cot.MaCot]"></a>
                            <span ng-switch-default ng-bind="::nghiphep[cot.MaCot]"></span>
                        </td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr>
                        <td colspan="100" class="text-center hidden-print">
                            <div st-pagination="" st-items-by-page="10" st-template="~/Scripts/angular/shared/smart-table/template/pagination.custom.html"></div>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </div>

        <div class="panel-footer hidden-print">
            <a class="btn btn-sm btn-primary" ng-if="ctrl.data.showButtonNew" data-toggle="collapse" data-target="#NghiPhepEditPopup" ng-click="ctrl.action.add()">Mới (F2)</a> &nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" ng-if="ctrl.data.showButtonXoaChon" ng-click="ctrl.action.deleteSelected()">Xóa chọn</a>
        </div>
    </div><!-- /panel danh sách nghỉ phép -->
    <!-- popup danh sách Người duyệt -->
    <div class="panel panel-primary ui-draggable collapse fade hidden-print" id="NhanVienListPopup" ng-controller="NhanVienListCtrl as ctrl" ng-init="ctrl.onInitView({controllerId:'NhanVienListPopupCtrl', showList:true,userInfo: @ViewBag.UserInfo })">
        <div class="panel-heading ui-draggable-handle">
            <div class="panel-title clearfix">
                <span>Danh sách người duyệt</span>
                <a class="btn btn-xs btn-primary pull-right" data-toggle="collapse" data-target="#NhanVienListPopup"><i class="fa fa-fw fa-remove"></i></a>
            </div>
        </div>
        <div class="panel-body" style="width:auto;height:auto;">
            <form class="form-inline ">
                <div class="form-group form-group-sm">
                    <span><i class="fa fa-fw fa-star-o"></i>Tìm kiếm người duyệt</span>
                </div>
                <div class="form-group form-group-sm">
                    <div class="input-group input-group-sm">
                        <input class="form-control" ng-model="ctrl.data.inputSearch.searchString" autofocus />
                        <span class="input-group-btn">
                            <button class="btn btn-sm btn-primary" ng-click="ctrl.action.getPage()"><i class="fa fa-fw fa-search"></i>Tìm kiếm</button>
                        </span>
                    </div>
                </div>
            </form>
            <table class="table table-bordered table-condensed table-hover table-responsive table-striped" st-table="ctrl.data.listNhanVien" st-pipe="ctrl.action.getPage">
                <thead>
                    <tr>
                        <th class="text-center" style="width: 50px;">Chọn</th>
                        <th class="text-center" style="width: 100px;" st-sort="Ma">Mã NV</th>
                        <th class="text-center" st-sort="Ten">Tên NV</th>

                    </tr>
                </thead>
                <tbody>
                    <tr ng-if="ctrl.data.isLoading">
                        <td colspan="100" class="text-center info">
                            <i class="fa fa-fw fa-spinner fa-pulse"></i>
                        </td>
                    </tr>
                    <tr ng-if="ctrl.data.error.code>0">
                        <td colspan="100" class="text-center danger" ng-bind="ctrl.data.error.message"></td>
                    </tr>
                    <tr ng-repeat="nhanvien in ctrl.data.listNhanVien" ng-class="(nhanvien.isSelected==true)?'info':''">
                        <td class="text-primary text-center" st-select-row="nhanvien" st-select-mode="multiple"><i class="fa fa-fw" ng-class="nhanvien.isSelected==true?'fa-check-square-o':'fa-square-o'"></i></td>
                        <td ng-repeat="cot in ctrl.data.listCot|filter:cot.chon=true" ng-switch="cot.MaCot">
                            <a href="javascript:void(0)" ng-switch-when="Ten" ng-bind="::nhanvien['Ho']+' '+nhanvien[cot.MaCot]"></a>
                            <a href="javascript:void(0)" ng-switch-when="Ma" ng-bind="::nhanvien[cot.MaCot]"></a>
                            <span ng-switch-default>{{::nhanvien[cot.MaCot]}}</span>
                        </td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr>
                        <td colspan="100" class="text-center hidden-print">
                            <div st-pagination="" st-items-by-page="10" st-template="~/Scripts/angular/shared/smart-table/template/pagination.custom.html"></div>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </div>
        <div class="panel-footer hidden-print">
            <a class="btn btn-sm btn-primary" ng-click="ctrl.action.apDung()">Áp dụng</a>&nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" data-toggle="collapse" data-target="#NhanVienListPopup">Đóng</a>
        </div>
    </div><!-- /popup danh sách nhân viên -->
    <!-- popup danh sách nhân viên 2 -->
