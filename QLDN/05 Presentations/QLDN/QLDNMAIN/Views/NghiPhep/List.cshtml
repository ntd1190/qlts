        </div>
    </div><!-- /popup danh sách nhân viên 3 -->
    <!-- popup Danh sách loại nghỉ phép -->
    <div class="panel panel-primary hidden-print collapse fade ui-draggable" ng-controller="LoaiNghiPhepListCtrl as ctrl" id="LoaiNghiPhepListPopup" ng-init="ctrl.onInitView({controllerId:'LoaiNghiPhepListPopupCtrl'})">
        <div class="panel-heading ui-draggable-handle">
            <div class="panel-title">
                <span>Danh sách loại nghỉ phép</span>
                <a class="btn btn-xs btn-primary pull-right" data-toggle="collapse" data-target="#LoaiNghiPhepListPopup"><i class="fa fa-fw fa-remove"></i></a>
            </div>
        </div>
        <div class="panel-body" style="width:auto;height:auto;">
            <table st-table="ctrl.data.listLoaiNghiPhep" st-pipe="ctrl.action.getPage" class="table table-bordered table-condensed table-hover table-responsive table-striped">
                <thead>
                    <tr>
                        <th class="text-center" style="width: 50px;">Chọn</th>
                        <th class="text-center" style="width: 200px;" st-sort="TenLoaiPhep">Tên loại phép</th>
                        <th class="text-center" style="width: 80px" st-sort="SoNgay">Số ngày</th>
                        <th class="text-center" st-sort="GhiChu">Ghi chú</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-if="ctrl.status.isLoading">
                        <td colspan="100" class="text-center">
                            <i class="fa fa-fw fa-spin fa-spinner"></i>
                        </td>
                    </tr>
                    <tr ng-repeat="loainghiphep in ctrl.data.listLoaiNghiPhep" ng-class="(loainghiphep.isSelected==true)?'info':''">
                        <td st-select-row="loainghiphep" st-select-mode="multiple" class="text-center"><i class="fa fa-fw text-primary" ng-class="(loainghiphep.isSelected==true)?'fa-check-square-o':'fa-square-o'"></i></td>
                        <td><a ng-bind="loainghiphep.TenLoaiPhep" href="javascript:void(0);"></a></td>
                        <td><span ng-bind="loainghiphep.SoNgay"></span></td>
                        <td><span ng-bind="loainghiphep.GhiChu"></span></td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr>
                        <td colspan="100" class="text-center">
                            <div st-pagination="" st-items-by-page="10" st-template="~/Scripts/angular/shared/smart-table/template/pagination.custom.html"></div>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </div>
        <div class="panel-footer">
            <button class="btn btn-sm btn-primary" ng-click="ctrl.action.apDung()">Áp dụng</button> &nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" data-toggle="collapse" data-target="#LoaiNghiPhepListPopup">Đóng</a>
        </div>
    </div><!-- /popup danh sách loại nghỉ phép -->
