
@{
    ViewBag.Title = "List";
}
<div class="container-fluid" ng-controller="MainCtrl as mainCtrl" ng-init="mainCtrl.onInitView()">
    <!-- panel tìm kiếm -->
    <div class="panel panel-default hidden-print" ng-controller="KhoHangHoaFilterCtrl as ctrl" ng-init="ctrl.onInitView('KhoHangHoaFilterCtrl')" keyboard="ctrl.keys">
        <div class="panel-heading">
            <div class="panel-title clearfix">
                <span><i class="fa fa-fw fa-star-o"></i>Kho hàng hóa</span>
                <div class="btn-group btn-group-sm pull-right">
                    <a class="btn btn-primary" data-toggle="collapse" data-target="#panelTimkiemCollapse"><i class="fa fa-fw fa-search"></i>Tìn kiếm (F3)</a>
                    <a class="btn btn-warning" data-toggle="collapse" data-target="#TuyChonCotPopup"><i class="fa fa-fw fa-cog"></i>Tùy chọn</a>
                </div>
            </div>
        </div>
        <div>
            <div class="collapse fade" id="panelTimkiemCollapse">
                <div class="panel-body">
                    <form class="form">
                        <div class="row form-group form-group-sm">
                            <label for="txtKhachHang" class="control-label col-sm-2">Từ khóa</label>
                            <div class="col-sm-10">
                                <input name="txtTuKhoa" type="text" class="form-control" ng-model="ctrl.data.TuKhoa" />
                            </div>
                        </div>
                        <div class="row form-group form-group-sm">
                            <label for="txtKhachHang" class="control-label col-sm-2">Hàng hóa</label>
                            <div class="col-sm-10">
                                <div class="form-control auto-height">
                                    <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.ClearlistKhoHangHoa()"><i class="fa fa-fw fa-remove"></i></a>
                                    <a href="javascript:void(0);" data-toggle="collapse" data-target="#KhoHangHoaListPopup">
                                        <div>
                                            <i class="fa fa-fw fa-search"></i>
                                            <span ng-repeat="hanghoa in ctrl.data.listKhoHangHoa" ng-bind="hanghoa.TenHangHoa+','" style="margin-right: 5px;"></span>
                                        </div>
                                            </a>
                                </div>
                            </div>
                        </div>
                        <div class="row form-group form-group-sm">
                            <label for="txtKhachHang" class="control-label col-sm-2">Nhóm</label>
                            <div class="col-sm-10">
                                <div class="form-control auto-height">
                                    <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.ClearlistKhoNhomHangHoa()"><i class="fa fa-fw fa-remove"></i></a>
                                    <a href="javascript:void(0);" data-toggle="collapse" data-target="#KhoNhomHangHoaListPopup">
                                        <div>
                                            <i class="fa fa-fw fa-search"></i>
                                            <span ng-repeat="nhom in ctrl.data.listKhoNhomHangHoa" ng-bind="nhom.TenNhom+','" style="margin-right: 5px;"></span>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="row form-group form-group-sm">
                            <label for="txtKhachHang" class="control-label col-sm-2">Loại</label>
                            <div class="col-sm-10">
                                <div class="form-control auto-height">
                                    <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.ClearlistKhoLoaiHangHoa()"><i class="fa fa-fw fa-remove"></i></a>
                                    <a href="javascript:void(0);" data-toggle="collapse" data-target="#KhoLoaiHangHoaListPopup">
                                        <div>
                                            <i class="fa fa-fw fa-search"></i>
                                            <span ng-repeat="loai in ctrl.data.listKhoLoaiHangHoa" ng-bind="loai.TenLoai+','" style="margin-right: 5px;"></span>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="row form-group form-group-sm">
                            <label for="txtKhachHang" class="control-label col-sm-2">Nhà cung cấp</label>
                            <div class="col-sm-10">
                                <div class="form-control auto-height">
                                    <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.clearListKhachHang()"><i class="fa fa-fw fa-remove"></i></a>
                                    <a href="javascript:void(0);" data-toggle="collapse" data-target="#KhachHangListPopup">
                                        <div>
                                            <i class="fa fa-fw fa-search"></i>
                                            <span class="" style="margin-right: 5px;" ng-bind="kh.TEN+','" ng-repeat="kh in ctrl.data.listKhachHang"></span>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="row form-group form-group-sm">
                            <label for="txtKhachHang" class="control-label col-sm-2">Hãng sản xuất</label>
                            <div class="col-sm-10">
                                <div class="form-control auto-height">
                                    <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.ClearlistKhoHangSanXuat()"><i class="fa fa-fw fa-remove"></i></a>
                                    <a href="javascript:void(0);" data-toggle="collapse" data-target="#KhoHangSanXuatListPopup">
                                        <div><i class="fa fa-fw fa-search"></i>
                                            <span ng-repeat="hsx in ctrl.data.listKhoHangSanXuat" ng-bind="hsx.TenHangSanXuat+','" style="margin-right: 5px;"></span>
                                        </div>
                                        
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="row form-group form-group-sm">
                            <label for="txtKhachHang" class="control-label col-sm-2">Nước sản xuất</label>
                            <div class="col-sm-10">
                                <div class="form-control auto-height">
                                    <a class="pull-right" href="javascript:void(0);" ng-click="ctrl.action.ClearlistKhoNuocSanXuat()"><i class="fa fa-fw fa-remove"></i></a>
                                    <a href="javascript:void(0);" data-toggle="collapse" data-target="#KhoNuocSanXuatListPopup">
                                        <div>
                                            <i class="fa fa-fw fa-search"></i>
                                            <span ng-repeat="nsx in ctrl.data.listKhoNuocSanXuat" ng-bind="nsx.TenNuoc+','" style="margin-right: 5px;"></span>
                                        </div>
                                        
                                    </a>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>

                <div class="panel-footer">
                    <a class="btn btn-sm btn-primary" ng-click="ctrl.action.search()">Tìm kiếm (F8)</a>&nbsp;&nbsp;&nbsp;
                    <a class="btn btn-sm btn-default" ng-click="ctrl.action.reset()">Thiết lặp lại</a>
                </div>
            </div>

        </div>
    </div>
    <!-- /panel tìm kiếm -->
    <!-- popup danh sách hàng hóa -->
    <div class="panel panel-primary hidden-print collapse fade ui-draggable" ng-controller="KhoHangHoaListPopCtrl as ctrl" id="KhoHangHoaListPopup" ng-init="ctrl.onInitView({controllerId:mainCtrl.controllerId.KhoHangHoaListPopup})">
        <div class="panel-heading ui-draggable-handle">
            <div class="panel-title">
                <span>Danh sách hàng hóa</span>
                <a class="btn btn-xs btn-primary pull-right" data-toggle="collapse" data-target="#KhoHangHoaListPopup"><i class="fa fa-fw fa-remove"></i></a>
            </div>
        </div>
        <div class="panel-body" style="width:auto; height:auto;">
            <form class="form-inline">
                <div class="form-group form-group-sm">
                    <span><i class="fa fa-fw fa-star-o"></i>Tìm kiếm hàng hóa</span>
                </div>
                <div class="form-group form-group-sm">
                    <div class="input-group input-group-sm">
                        <input class="form-control" ng-model="ctrl.data.searchString" autofocus />
                        <span class="input-group-btn">
                            <button class="btn btn-sm btn-primary" ng-click="ctrl.action.getPage()"><i class="fa fa-fw fa-search"></i>Tìm kiếm</button>
                        </span>
                    </div>
                </div>
            </form>
        </div>
        <div class="table-responsive">
            <table st-table="ctrl.data.listKhoHangHoa" st-pipe="ctrl.action.getPage" class="table table-bordered table-condensed table-responsive table-hover ">
                <thead class="bg-default text-primary">
                    <tr>
                        <th class="text-center" style="width: 50px;">Chọn</th>
                        <th class="text-center" style="width: 100px;" st-sort="MaHangHoa">Mã</th>
                        <th class="text-center" style="width: 300px" st-sort="TenHangHoa">Tên</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-if="ctrl.status.isLoading">
                        <td colspan="100" class="text-center">
                            <i class="fa fa-fw fa-spin fa-spinner"></i>
                        </td>
                    </tr>
                    <tr ng-repeat="nsx in ctrl.data.listKhoHangHoa" ng-class="(nsx.isSelected==true)?'info':''">
                        <td st-select-row="nsx" st-select-mode="multiple" class="text-center"><i class="fa fa-fw text-primary" ng-class="(nsx.isSelected==true)?'fa-check-square-o':'fa-square-o'"></i></td>
                        <td><a ng-bind="nsx.MaHangHoa" ng-click="ctrl.action.edit(nsx.HangHoaId)" href="javascript:void(0);"></a></td>
                        <td><a ng-bind="nsx.TenHangHoa" ng-click="ctrl.action.edit(nsx.HangHoaId)" href="javascript:void(0);"></a></td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr>
                        <td colspan="100" class="text-center">
                            <div st-pagination="" st-items-by-page="10" st-template="~/Scripts/angular/shared/smart-table/template/pagination.custom.html"></div>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </div>
        <div class="panel-footer">
            <button class="btn btn-sm btn-primary" ng-click="ctrl.action.apDung()">Áp dụng</button>
            <button class="btn btn-sm btn-default" data-toggle="collapse" data-target="#KhoHangHoaListPopup">Đóng</button>
        </div>
    </div>
    <!-- /popup danh sách -->
    <!-- popup danh sách nhóm -->
