
@if (ViewBag.Html != null && (bool)ViewBag.Html == true)
{
    <div ng-controller="CoSoCombobox as ctrl" ng-init="ctrl.onInitView(@(ViewBag.onInitView))">
        <ui-select  id="{{ctrl.NameId}}"  ng-model="ctrl.data.CoSo" ng-change="ctrl.action.OnClickSelect()" theme="bootstrap">
            <ui-select-match  tabindex='1' placeholder="Chọn cơ sở">{{$select.selected.TenCoSo}}</ui-select-match>
            <ui-select-choices repeat="item in ctrl.data.CoSoListDisplay | filter: $select.search">
                <div ng-bind-html="item.TenCoSo | highlight: $select.search"></div>
            </ui-select-choices>
        </ui-select>

    </div>

<link href="~/lib/Select/Select.css" rel="stylesheet" />
}
else if (ViewBag.ImportStyles != null && (bool)ViewBag.ImportStyles == true) { }
else if (ViewBag.SetupStyles != null && (bool)ViewBag.SetupStyles == true) { }
else if (ViewBag.ImportScripts != null && (bool)ViewBag.ImportScripts == true) { }
else if (ViewBag.SetupScripts != null && (bool)ViewBag.SetupScripts == true) { }
else if (ViewBag.ImportAngular != null && (bool)ViewBag.ImportAngular == true)
{
    <script src="~/Scripts/angular/CoSo/controller/Combobox.js"></script>
    <script src="~/Scripts/angular/CoSo/service/CoSoService.js"></script>
}







