@if (ViewBag.Html != null && (bool)ViewBag.Html == true)
{
    <ui-select ng-model="ctrl.data.CoSo" data-ng-disabled="disabled" on-select="ctrl.action.onSelected($item, $model)" theme="bootstrap" id="{{ctrl.NameId}}">
        <ui-select-match placeholder="{{$select.selected.TenCoSo || 'Chọn cơ sở'}}">{{$select.selected.TenCoSo}}</ui-select-match>
        <ui-select-choices repeat="item in ctrl.data.CoSoList | filter: $select.search" refresh="ctrl.action.search($select)" refresh-delay="200" position="auto">
            <div style="border-bottom: #ccc 1px solid;">
                <b><span class="text-danger" ng-bind-html="item.MaCoSo | highlight: $select.search"></span></b>: <b style="color:#000"><strong class="tt-highlight"><span ng-bind-html="item.TenCoSo | highlight: $select.search"></span></strong></b>
            </div>
        </ui-select-choices>
    </ui-select>

}
else if (ViewBag.ImportStyles != null && (bool)ViewBag.ImportStyles == true)
{
}
else if (ViewBag.SetupStyles != null && (bool)ViewBag.SetupStyles == true) { }
else if (ViewBag.ImportScripts != null && (bool)ViewBag.ImportScripts == true) { }
else if (ViewBag.SetupScripts != null && (bool)ViewBag.SetupScripts == true) { }
else if (ViewBag.ImportAngular != null && (bool)ViewBag.ImportAngular == true)
{
    <script src="~/Scripts/angular/CoSo/directive/cboCoSo.js"></script>
    <script src="~/Scripts/angular/CoSo/service/CoSoService.js"></script>
}
