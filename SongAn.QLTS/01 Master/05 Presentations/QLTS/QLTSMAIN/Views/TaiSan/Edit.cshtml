                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" ng-model="ctrl.data.TTCK.NopNganSach" number-format ng-keypress="ctrl.action.keyPressTTCK($event)" data-name="TTCK_NopNganSach" data-next="TTCK_DeLaiDonVi" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.TTCK_DeLaiDonVi?'has-error':''">
                                        <span class="col-sm-4">Để lại đơn vị</span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" ng-model="ctrl.data.TTCK.DeLaiDonVi" number-format ng-keypress="ctrl.action.keyPressTTCK($event)" data-name="TTCK_DeLaiDonVi" data-next="TTCK_HHCK" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.TTCK_HHCK?'has-error':''">
                                        <span class="col-sm-4">Giá trị HHCK</span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" ng-model="ctrl.data.TTCK.HHCK" number-format ng-keypress="ctrl.action.keyPressTTCK($event)" data-name="TTCK_HHCK" data-next="TTCK_NhaCungCapId" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.TTCK_NhaCungCapId?'has-error':''">
                                        <span class="col-sm-4">Nhà cung cấp</span>
                                        <div class="col-sm-8">
                                            <cbo-nha-cung-cap data-value="ctrl.data.TTCK.NhaCungCapId" data-config="{userInfo:@(ViewBag.userInfo)}" template-url="~/NhaCungCap/showview?viewname=cboNhaCungCap" data-name="TTCK_NhaCungCapId"></cbo-nha-cung-cap>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div><!--/ #ThongTinCongKhai -->
            </div>
        </div>
        <div class="panel-footer">
            <span ng-if="ctrl.action.checkQuyenTacVu('N')||ctrl.action.checkQuyenTacVu('M')">
                <button type="button" class="btn btn-sm btn-primary" ng-click="ctrl.action.save()"><i class="fa fa-save"></i> Lưu (F8)</button>&nbsp;&nbsp;&nbsp;
            </span>
            <span ng-if="ctrl.action.checkQuyenTacVu('D')">
                <button type="button" class="btn btn-sm btn-danger" ng-click="ctrl.action.removeList()"><i class="fa fa-trash-o"></i> Xóa</button>&nbsp;&nbsp;&nbsp;
            </span>
            <a class="btn btn-sm btn-default" href="javascript:void(0)" ng-click="ctrl.action.In()"><i class="fa fa-print"></i> In</a>&nbsp;&nbsp;&nbsp;
            <a class="btn btn-sm btn-default" ng-click="ctrl.action.goBack()"><i class="fa fa-arrow-left"></i> Quay lại</a>&nbsp;&nbsp;&nbsp;
        </div>
    </div>
</div>
