@{
    /*****************************************************************************
  1. Create Date : 2017.06.16
  2. Creator     : Nguyen Thanh Binh
  3. Description : khophieunhap/edit
  4. History     : 2017.06.16 (Nguyen Thanh Binh) - thiết kế giao diện
                   2017.06.28 (Nguyen Thanh Binh) - update, thêm thông tin ngày nhập, người nhận hang, thủ kho
  *****************************************************************************/
    ViewBag.Title = "Thông tin tài sản";
    ViewBag.id = ViewBag.id ?? 0;
    ViewBag.userInfo = ViewBag.userInfo ?? "{ }";
    ViewBag.isEdit = ViewBag.isEdit ?? 0;
}
<div class="container-fluid">
    <div class="panel panel-default with-nav-tabs" ng-controller="TaiSanEditCtrl as ctrl" keyboard="ctrl.keys" ng-init="ctrl.onInitView({isEdit:@(ViewBag.isEdit),TaiSanId:@(ViewBag.id),userInfo:@(ViewBag.userInfo),linkUrl:'@(Url.Content("~/taisan/"))'})">
        <div class="panel-heading">
            <ul class="nav nav-tabs">
                <li class="active"><a href="javascript:void(0);" data-target="#ThongTinChung" data-toggle="tab">Thông tin chung</a></li>
                <li><a href="javascript:void(0);" data-target="#ThongTinKeKhai" data-toggle="tab">Thông tin kê khai</a></li>
                <li><a href="javascript:void(0);" data-target="#ThongTinCongKhai" data-toggle="tab">Thông tin công khai</a></li>
            </ul>
        </div>
        <div class="panel-body" style="min-height: 720px;">
            <div class="tab-content">
                <!-- #ThongTinChung -->
                <div class="tab-pane fade in active" id="ThongTinChung">
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_MaTaiSan?'has-error':''">
                                <span>Mã tài sản</span><span class="text-danger" data-toggle="tooltip" title="Bắt buộc nhập !">&nbsp;*</span>
                                <input type="text" class="form-control" role="textbox" data-toggle="popover" data-trigger="focus" data-placement="bottom" data-content="{{ctrl.error.TaiSan_MaTaiSan}}"
                                       ng-model="ctrl.data.TaiSan.MaTaiSan" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="TaiSan_MaTaiSan" data-next="TaiSan_TenTaiSan" autofocus />
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_TenTaiSan?'has-error':''">
                                <span>Tên tài sản</span><span class="text-danger" data-toggle="tooltip" title="Bắt buộc nhập !">&nbsp;*</span>
                                <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.TenTaiSan" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="TaiSan_TenTaiSan" data-next="TaiSan_DonViTinh" />
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_DonViTinh?'has-error':''">
                                <span>Đơn vị tính</span>
                                <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.DonViTinh" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="TaiSan_DonViTinh" data-next="TaiSan_DuAnId" />
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_DuAnId?'has-error':''">
                                <span>Dự án</span>
                                <cbo-du-an data-value="ctrl.data.TaiSan.DuAnId" data-config="{userInfo:@(ViewBag.userInfo),NameId:'DuAnId'}" template-url="~/duan/showview?viewname=cboduan" data-name="TaiSan_DuAnId"></cbo-du-an>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_SoQDTC?'has-error':''">
                                <span>Số QĐTC</span>
                                <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.SoQDTC" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="TaiSan_SoQDTC" data-next="TaiSan_PhuongThucId" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_PhuongThucId?'has-error':''">
                                <span>Phương thức HT</span>
                                <cbo-phuong-thuc data-value="ctrl.data.TaiSan.PhuongThucId" data-config="{userInfo:@(ViewBag.userInfo)}" template-url="~/phuongthuc/showview?viewname=cbophuongthuc" data-name="TaiSan_PhuongThucId"></cbo-phuong-thuc>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_LoaiId?'has-error':''">
                                <span>Loại tài sản<span class="text-danger" data-toggle="tooltip" title="Bắt buộc nhập !">&nbsp;*</span></span>
                                <cbo-loai-tai-san on-Selected="ctrl.action.getLoaiTaiSan(data)" data-value="ctrl.data.TaiSan.LoaiId" data-config="{userInfo:@(ViewBag.userInfo)}" template-url="~/loaitaisan/showview?viewname=cboloaitaisan" data-name="TaiSan_LoaiId"></cbo-loai-tai-san>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_NamSanXuat?'has-error':''">
                                <span>Năm Sản xuất</span>
                                <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.NamSanXuat" maxlength="4" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="TaiSan_NamSanXuat" data-next="TaiSan_NuocSanXuatId" />
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_NuocSanXuatId?'has-error':''">
                                <span>Nước Sản xuất<span class="text-danger" data-toggle="tooltip" title="Bắt buộc nhập !">&nbsp;*</span></span>
                                <cbo-nuoc-san-xuat data-value="ctrl.data.TaiSan.NuocSanXuatId" data-config="{userInfo:@(ViewBag.userInfo)}" template-url="~/nuocsanxuat/showview?viewname=cbonuocsanxuat" data-name="TaiSan_NuocSanXuatId"></cbo-nuoc-san-xuat>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_HangSanXuatId?'has-error':''">
                                <span>Hãng sản xuất</span>
                                <cbo-hang-san-xuat data-value="ctrl.data.TaiSan.HangSanXuatId" data-config="{userInfo:@(ViewBag.userInfo)}" template-url="~/hangsanxuat/showview?viewname=cbohangsanxuat" data-name="TaiSan_HangSanXuatId"></cbo-hang-san-xuat>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group form-group-sm" ng-class="ctrl.error.TaiSan_NhanHieu?'has-error':''">
                                <span>Nhãn hiệu</span>
                                <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.NhanHieu" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="TaiSan_NhanHieu" data-next="listNguyenGia_NguonNganSachId0" />
                            </div>
                        </div>
                    </div>
                    <div class="panel-heading" style="padding: 10px 15px;border: 1px solid #ddd;;background-color: #f5f5f5;" ng-if="ctrl.action.checkQuyenTacVu('N')||ctrl.action.checkQuyenTacVu('M')">
                        <button type="button" class="btn btn-sm btn-primary" ng-click="ctrl.action.addNguyenGia()"><i class="fa fa-fw fa-plus"></i>Thêm (F2)</button>
                    </div>
                    <div class="table-responsive">
                        <table class="table table-bordered table-condensed table-responsive table-hover ">
                            <thead class="text-primary">
                                <tr>
                                    <th class="text-center" style="width: 50px; vertical-align:middle">#</th>
                                    <th class="text-center" style="width: 50px; vertical-align:middle">Xóa</th>
                                    <th class="text-center">Nguồn hình thành<span class="text-danger" data-toggle="tooltip" title="Bắt buộc nhập !">&nbsp;*</span></th>
                                    <th class="text-center" style="width: 200px">Giá trị<span class="text-danger" data-toggle="tooltip" title="Bắt buộc nhập !">&nbsp;*</span></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="row in ctrl.data.listNguyenGia" ng-class="row.isError ? 'danger':''">
                                    <td class="text-center">{{$index + 1}}</td>
                                    <td class="text-center">
                                        <a class="btn btn-xs btn-danger" ng-click="ctrl.action.removeNguyenGia($index)" ng-if="ctrl.action.checkQuyenTacVu('N')||ctrl.action.checkQuyenTacVu('M')"><i class="fa fa-remove"></i></a>
                                    </td>
                                    <td class="form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <cbo-nguon-ngan-sach data-name="listNguyenGia_NguonNganSachId{{$index}}" data-value="row.NguonNganSachId" data-config="{userInfo:@(ViewBag.userInfo)}" template-url="~/nguonngansach/showview?viewname=cbonguonngansach"></cbo-nguon-ngan-sach>
                                    </td>
                                    <td class="form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <input type="text" class="form-control text-right" ng-model="row.GiaTri" number-format data-name="listNguyenGia_GiaTri{{$index}}" data-next="listNguyenGia_NguonNganSachId{{$index+1}}" ng-keypress="ctrl.action.keyPressNganSach($event,$index)" />
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="3" class="text-right">Tổng cộng</td>
                                    <td class="text-right"><span ng-bind="ctrl.data.NguyenGia=(ctrl.data.listNguyenGia|sumOfValue:'GiaTri')|number:'':'':2"></span></td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="panel panel-default">
                                <div class="panel-heading"><h5>Thông tin chung</h5></div>
                                <div class="panel-body">
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Ngày mua<span class="text-danger" data-toggle="tooltip" title="Bắt buộc nhập !">&nbsp;*</span></span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control datetimepicker date" ng-model="ctrl.data.TaiSan.NgayMua" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="NhanHieu" data-next="TaiSan_NhanHieu" />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Năm theo dõi</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.NamTheoDoi"></span>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Giá trị còn lại</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.GiaTriConLai|number:'':'':2"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                        <div class="col-sm-4">
                            <div class="panel panel-default">
                                <div class="panel-heading"><h5>Thông tin hao mòn</h5></div>
                                <div class="panel-body">
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Ngày BĐ tính HM</span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control datetimepicker date" ng-model="ctrl.data.TaiSan.NgayBDHaoMon" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="NhanHieu" data-next="TaiSan_NhanHieu" />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Ngày tính hao mòn</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.NgayBDHaoMon|date:'dd/MM/yyyy'"></span>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Số năm sử dụng</span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.SoNamSuDung" number-format ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="NhanHieu" data-next="TaiSan_NhanHieu" />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Tỷ lệ HM (%)</span>
                                        <div class="col-sm-8">
                                            <input class="form-control" ng-model="ctrl.data.TaiSan.TyLeHaoMon" number-format />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Hao mòn năm</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.HaoMonNam|number:'':'':2"></span>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Hao mòn lũy kế</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.HaoMonLuyKe|number:'':'':2" />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Số năm SD còn lại</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.SoNamSDConLai|number:'':'':2"></span>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Ngày KT tính HM</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.NgayKTHaoMon|date:'dd/MM/yyyy'"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="panel panel-default">
                                <div class="panel-heading"><h5>Thông tin khấu hao</h5></div>
                                <div class="panel-body">
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Ngày tính khấu hao</span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control datetimepicker date" ng-model="ctrl.data.TaiSan.NgayBDKhauHao" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="NhanHieu" data-next="TaiSan_NhanHieu" />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Kỳ tính khấu hao</span>
                                        <div class="col-sm-8">
                                            @*<input type="text" class="form-control" ng-model="ctrl.data.TaiSan.KyTinhKhauHao" ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="NhanHieu" data-next="TaiSan_NhanHieu" />*@
                                            <select class="form-control" ng-model="ctrl.data.TaiSan.KyTinhKhauHao">
                                                <option value="Tháng">Tháng</option>
                                                <option value="Quý">Quý</option>
                                                <option value="Năm">Năm</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Giá trị tính KH</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.NguyenGia|number"></span>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Số kỳ KH</span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.SoKyKhauHao" number-format ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="NhanHieu" data-next="TaiSan_NhanHieu" />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Số kỳ còn lại</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.SoKyConLai|number"></span>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Tỷ lệ KH (%)</span>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" ng-model="ctrl.data.TaiSan.TyLeKhauHao" number-format ng-keypress="ctrl.action.keyPressTaiSan($event)" data-name="NhanHieu" data-next="TaiSan_NhanHieu" />
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Khấu hao kỳ</span>
                                        <div class="col-sm-8">
                                            <span type="text" class="form-control" ng-bind="ctrl.data.TaiSan.KhauHaoKy|number"></span>
                                        </div>
                                    </div>
                                    <div class="row form-group form-group-sm" ng-class="ctrl.error.messge?'has-error':''">
                                        <span class="col-sm-4">Khấu hao lũy kế</span>
                                        <div class="col-sm-8">
                                            <span class="form-control" ng-bind="ctrl.data.TaiSan.KhauHaoLuyKe|number"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </div>
                </div><!--/ #ThongTinChung -->
                <!-- #ThongTinKeKhai -->
                <div class="tab-pane fade" id="ThongTinKeKhai">
                    <div class="row">
                        <div class="col-sm-12">
                            <label>
                                <input type="radio" ng-checked="true" ng-model="ctrl.data.TaiSan.LoaiKeKhai" value="0" />
                                <span>Không kê khai</span>
                            </label>
                            <label>
                                <input type="radio" ng-model="ctrl.data.TaiSan.LoaiKeKhai" value="1" />
                                <span>Đất</span>
                            </label>
                            <label>
                                <input type="radio" ng-model="ctrl.data.TaiSan.LoaiKeKhai" value="2" />
                                <span>Nhà</span>
                            </label>
                            <label>
                                <input type="radio" ng-model="ctrl.data.TaiSan.LoaiKeKhai" value="3" />
                                <span>Ô tô</span>
                            </label>
                            <label>
                                <input type="radio" ng-model="ctrl.data.TaiSan.LoaiKeKhai" value="4" />
                                <span>Tài sản trên 500 triệu</span>
                            </label>
                        </div>
                    </div>
                    <div class="collapse fade" ng-class="ctrl.data.TaiSan.LoaiKeKhai==0?'in':''"></div>
                    <!-- Kê khai đất -->
                    <div class="collapse fade" ng-class="ctrl.data.TaiSan.LoaiKeKhai==1?'in':''">

                        <div class="panel panel-default">
                            <div class="panel-heading"><h5>Thông tin kê khai</h5></div>
                            <div class="panel-body">
                                <div class="row form-group form-group-sm" ng-class="ctrl.error.TTKK_Dat_DiaChi?'has-error':''">
                                    <span class="col-sm-1">Địa chỉ <span class="text-danger">*</span></span>
                                    <div class="col-sm-11">
                                        <input type="text" class="form-control" ng-model="ctrl.data.TTKK_Dat.DiaChi" ng-keypress="ctrl.action.keyPressTTKK($event)" data-name="TTKK_Dat_DiaChi" data-next="TTKK_Dat_GiayTo" />
                                    </div>
                                </div>
                                <div class="row form-group form-group-sm" ng-class="ctrl.error.TTKK_Dat_GiayTo?'has-error':''">
                                    <span class="col-sm-1">Giấy tờ kèm theo</span>
                                    <div class="col-sm-11">
                                        <textarea class="form-control" ng-model="ctrl.data.TTKK_Dat.GiayTo" data-name="TTKK_Dat_GiayTo"></textarea>
                                    </div>
                                </div>
                                <div class="row form-group form-group-sm" ng-class="ctrl.error.TTKK_Dat_DienTich?'has-error':''">
                                    <span class="col-sm-1">Diện tích</span>
                                    <div class="col-sm-11">
                                        <span class="form-control" ng-bind="ctrl.data.TTKK_Dat.DienTich|number:'':'':2" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading"><h5>Hiện trạng sử dụng</h5></div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="row form-group form-group-sm" ng-class="ctrl.error.TTKK_Dat_LamTruSo?'has-error':''">
                                            <span class="col-sm-4">Làm TSLV <span class="text-danger">*</span></span>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" ng-model="ctrl.data.TTKK_Dat.LamTruSo" number-format ng-keypress="ctrl.action.keyPressTTKK($event)" data-name="TTKK_Dat_LamTruSo" data-next="TTKK_Dat_ChoThue" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="row form-group form-group-sm" ng-class="ctrl.error.TTKK_Dat_ChoThue?'has-error':''">
