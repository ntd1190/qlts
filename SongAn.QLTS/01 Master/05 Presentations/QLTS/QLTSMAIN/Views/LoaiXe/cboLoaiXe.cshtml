
@if (ViewBag.Html != null && (bool)ViewBag.Html == true)
{
    <ui-select ng-model="ctrl.data.LoaiXe" data-ng-disabled="disabled" on-select="ctrl.action.onSelected($item, $model)" theme="bootstrap" id="{{ctrl.NameId}}">
        <ui-select-match placeholder="{{$select.selected.TenLoaiXe || 'Chọn hiện loại xe'}}">{{$select.selected.NoiDung}}</ui-select-match>
        <ui-select-choices repeat="item in ctrl.data.LoaiXeListDisplay | filter: $select.search" refresh="ctrl.action.search($select)" refresh-delay="1000" position="down">
            <div ng-if="!item.LoaiXeId">.</div>
            <div ng-if="item.LoaiXeId">
                <span ng-bind-html="item.NoiDung | highlight: $select.search"></span>
            </div>
        </ui-select-choices>
    </ui-select>

}
else if (ViewBag.ImportStyles != null && (bool)ViewBag.ImportStyles == true)
{
}
else if (ViewBag.SetupStyles != null && (bool)ViewBag.SetupStyles == true) { }
else if (ViewBag.ImportScripts != null && (bool)ViewBag.ImportScripts == true) { }
else if (ViewBag.SetupScripts != null && (bool)ViewBag.SetupScripts == true) { }
else if (ViewBag.ImportAngular != null && (bool)ViewBag.ImportAngular == true)
{
    <script src="~/Scripts/angular/LoaiXe/directive/cboLoaiXe.js"></script>
    <script src="~/Scripts/angular/LoaiXe/service/LoaiXeService.js"></script>
}
