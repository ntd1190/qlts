@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div id="products-feature">
        <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active">
                <a href="#sanPhamMoi1" aria-controls="home" role="tab"
                   data-toggle="tab" title="Sản phẩm liên quan">
                    Sản phẩm cũng được mua
                </a>
            </li>

        </ul>
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane active fade in" id="sanPhamMoi1">
                <div class="product-carousel shop clearfix">
                    @foreach (var item in Model)
                    {
                        @Html.Partial("_ProductList", item)
                    }
                </div>
            </div>
        </div>
    </div>
}